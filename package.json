  "license": "MIT",
  "author": "Vadim Kazakov <vkazakov@sugarcrm.com>",

  "dependencies": {
    "codemod-cli": "^3.2.0"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^26.6.3",
